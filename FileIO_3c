#include<stdio.h>
int main()
{
    FILE *fptr;
    // append
    fptr = fopen("test.txt", "a");
    fprintf(fptr, "%c", ' ');
    fprintf(fptr, "%c", 'n');
    fprintf(fptr, "%c", 'a');
    fprintf(fptr, "%c", 'g');
    fprintf(fptr, "%c", 'a');
    fprintf(fptr, "%c", 'n');
    fprintf(fptr, "%c", 'i');

    // OR
    // char ch[] = " nagani";
    // fprintf(fptr, "%s", ch);

    fclose(fptr);

    return 0;
}
